<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.AreaMapper">

    <select id="selectAllArea" resultType="com.pojo.Area">
        select * from area
    </select>
    <select id="selectAllAreaByFather" resultType="com.pojo.Area">
        select  * from area where father = #{code}
    </select>
</mapper>